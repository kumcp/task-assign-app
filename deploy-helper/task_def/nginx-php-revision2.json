[
    {
        "name": "php-fpm",
        "image": "666402361323.dkr.ecr.ap-southeast-1.amazonaws.com/php-fpm:latest",
        "cpu": 0,
        "portMappings": [
            {
                "containerPort": 9000,
                "hostPort": 9000,
                "protocol": "tcp"
            }
        ],
        "essential": true,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-create-group": "true",
                "awslogs-group": "/ecs/nginx-php",
                "awslogs-region": "ap-southeast-1",
                "awslogs-stream-prefix": "ecs"
            },
            "secretOptions": []
        },
        "systemControls": []
    },
    {
        "name": "nginx",
        "image": "666402361323.dkr.ecr.ap-southeast-1.amazonaws.com/built-nginx:latest",
        "cpu": 0,
        "links": [],
        "portMappings": [
            {
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp"
            }
        ],
        "essential": true,
        "entryPoint": [],
        "command": [],
        "environment": [
            {
                "name": "NGINX_HOST",
                "value": "localhost"
            },
            {
                "name": "NGINX_PORT",
                "value": "80"
            }
        ],
        "dependsOn": [
            {
                "containerName": "php-fpm",
                "condition": "START"
            }
        ]
    }
]
